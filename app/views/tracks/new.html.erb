<div class = "row">
<div class="col-md-6 col-md-offset-3">
<%= form_for(@track, multipart: true) do |f|%>


  <div class="form-group">
    <%= f.label :task %>
    <%= f.text_field :task, class: "form-control"  %>



  </div>

  <div class="form-group">
    <%= f.submit 'Start your task', class: "form-control btn btn-danger" %>

  </div>
  <% end %>
  </div>

</div>
</div>




